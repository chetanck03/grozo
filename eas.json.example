{
  "cli": {
    "version": ">= 12.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": ""
      },
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleDebug",
        "resourceClass": "medium"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": ""
      },
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease",
        "resourceClass": "medium"
      }
    },
    "production": {
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": ""
      },
      "ios": {
        "resourceClass": "medium"
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "medium"
      }
    },
    "production-aab": {
      "env": {
        "EXPO_PUBLIC_GEMINI_API_KEY": ""
      },
      "android": {
        "buildType": "app-bundle",
        "resourceClass": "medium"
      }
    }
  },
  "submit": {
    "production": {}
  }
}